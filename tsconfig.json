{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@Assets/*": ["src/Assets/*"],
      "@Components/*": ["src/Components/*"],
      "@Config/*": ["src/Config/*"],
      "@Layouts/*": ["src/Layouts/*"],
      "@Pages/*": ["src/Pages/*"],
      "@Services/*": ["src/Services/*"],
      "@Styles/*": ["src/Styles/*"]
    }
  },
  "include": [
    "src"
  ]
}
